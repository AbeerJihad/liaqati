@model List<VmHealthyRecipe>
<div>
    <div class="d-flex justify-content-between align-items-center px-3">
        <div class="d-flex align-items-center">
            <div style="width:5px;background-color:#be8adc;height:40px;"></div>
            <h4 class="fw-bold m-0 m-2 ">الأكثر مشاهدة</h4>
        </div>
        <div>
            <i class="bi bi-arrow-left h4"></i>
        </div>
    </div>
    <div class="row  row-cols-1 row-cols-md-2 row-cols-lg-4 p-0 g-0">

        @foreach (var item in Model)
        {
            <partial name="_HealthyRecipeCard" model="item" />
        }
    </div>

</div>