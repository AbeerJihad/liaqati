@model List<HealthyRecipe>
@if (Model.Count > 0)
{

    <h4 class="fw-bold m-0 m-2 ">    @ViewData["ComponentTitle"]</h4>
    <div class="row  row-cols-1 flex-column p-0 g-0">
        @foreach (var item in Model)
        {
            <partial name="_HealthyRecipeSmallCard" model="item" />
        }

    </div>
}